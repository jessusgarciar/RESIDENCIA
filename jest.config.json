{
  "transform": {},
  "testEnvironment": "node",
  "moduleFileExtensions": ["js", "json"],
  "testMatch": ["**/__tests__/**/*.test.js", "**/?(*.)+(spec|test).js"],
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js",
    "!src/database.js"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": ["text", "lcov", "html"]
}
